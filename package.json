  "dependencies": {
    "@aws-sdk/client-sts": "^3.600.0",
    "@smithy/node-http-handler": "^3.0.0",
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "debug": "^4.3.6",
    "hpagent": "^1.2.0",
    "ini": "^4.1.3",
    "inquirer": "^8.2.6",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "puppeteer": "^21.11.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/cheerio": "^0.22.35",
    "@types/debug": "^4.1.12",
    "@types/ini": "^4.1.1",
    "@types/inquirer": "^9.0.07",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"  },
    "pkg": {
      "assets": "node_modules/vm2/**/*",
      "targets": [ "node16-alpine-x64" ],
      "outputPath": "dist"
